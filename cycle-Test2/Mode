public class Mode {
    public static void main(String[] args) {
        int a[] = {1, 2, 3, 4, 5, 6, 7, 7, 8};
        int  maxcount = 0, maxvalue = 0, i, j;

        for (i = 0; i < a.length; i++) {
            int count = 0;
            for (j = 0; j < a.length; j++) {
                if (a[i] == a[j]) {
                    count = count + 1;
                }
            }
            if (count > maxcount) {
                maxcount = count;
                maxvalue = a[i];
            }
        }

        System.out.println("Mode of the given array is " + maxvalue);
    }
}
