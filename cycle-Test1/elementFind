import java.util.Scanner;
public class ElementFind {
    public static void main(String[] args){
        int a[]={5,66,77,34,89,88,88,12,45,98,7};
        Scanner b=new Scanner(System.in);
        System.out.print("enter element to find in the array:");
        int num=b.nextInt();
        int count=0;
        for(int i=0;i<a.length;i++){
            if(a[i]==num){
                count=count+1;
            }

        }
        if(count>=1){
            System.out.println(num+" present in the array "+count+" times");

        }
        else{
            System.out.println(num+" Not present in the array");
        }


    }
}
