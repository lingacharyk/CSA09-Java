import java.util.LinkedList;

public class collect {
    public static void main(String[] args){
        LinkedList <String> l= new LinkedList<String>();
        l.add("abc");
        l.add("cde");
        l.add("fgh");
        System.out.println(l);
        l.remove(1);
        System.out.println("After removing:"+l);

    }
}
