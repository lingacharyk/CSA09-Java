public class fact_rec {
    public static void main(String[] args) {
        int num = 6;
        int result = factorial(num);
        System.out.println("Factorial of " + num + " is: " + result);
    }

    static int factorial(int n) {
        if (n == 0 || n == 1) {
            return 1;
        } else {
            return n * factorial(n - 1);
        }
    }
}
