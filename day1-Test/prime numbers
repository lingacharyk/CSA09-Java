public class prim {
    public static void main(String[] args) {
        int a = 1, b = 50;

        System.out.print("Prime numbers between " + a + " and " + b + " are: ");
        for (int i = a; i <= b; i++) {
            if (isPrime(i)) {
                System.out.print(i + " ");
            }
        }
    }

    static boolean isPrime(int num) {
        if (num <= 1) {
            return false;
        }

        for (int j = 2; j <= Math.sqrt(num); j++) {
            if (num % j == 0) {
                return false;
            }
        }

        return true;
    }
}
